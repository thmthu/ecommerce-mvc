<li class="page-item <%= currentPage == 1 ? 'disabled' : '' %>">
    <a class="page-link" href="#" onclick="loadPage(<%= currentPage - 1 %>, '<%= sortBy %>')" aria-label="Previous">
        <span aria-hidden="true">&laquo;</span>
        <span class="sr-only">Previous</span>
    </a>
    </li>
    <li class="page-item <%= currentPage == 1 ? 'active' : '' %>">
    <a class="page-link" href="#" onclick="loadPage(1, '<%= sortBy %>')">1</a>
    </li>
    <% if (totalPages > 1) { %>
    <% if (totalPages > 2) { %> 
        <% if (currentPage > 2 && currentPage < totalPages - 1) { %>
        <li class="page-item disabled">
            <a class="page-link" href="#">...</a>
        </li>
        <% } else { %> 
        <li class="page-item <%= currentPage == 2 ? 'active' : '' %>">
            <a class="page-link" href="#" onclick="loadPage(2, '<%= sortBy %>')">2</a>
        </li>
        <% } %>
        <% if (totalPages > 3) { %> 
        <% if (totalPages > 4 && (currentPage <= 2 || currentPage >= totalPages - 1)) { %> 
            <li class="page-item disabled">
            <a class="page-link" href="#">...</a>
            </li>
        <% } else if (totalPages > 4) { %>
            <li class="page-item active disabled">
            <a class="page-link" href="#"><%= currentPage %></a>
            </li>
        <% } %>
        <% if (currentPage > 2 && currentPage < totalPages - 1) { %>
            <li class="page-item disabled">
            <a class="page-link" href="#">...</a>
            </li>
        <% } else { %> 
            <li class="page-item <%= currentPage == totalPages - 1 ? 'active' : '' %>">
            <a class="page-link" href="#" onclick="loadPage(<%= totalPages - 1 %>, '<%= sortBy %>')"><%= totalPages - 1 %></a>
            </li>
        <% } %>
        <% } %>
    <% } %>
    <li class="page-item <%= currentPage == totalPages ? 'active' : '' %>">
        <a class="page-link" href="#" onclick="loadPage(<%= totalPages %>, '<%= sortBy %>')"><%= totalPages %></a>
    </li>
    <% } %>
    <li class="page-item <%= currentPage == totalPages ? 'disabled' : '' %>">
    <a class="page-link" href="#" onclick="loadPage(<%= currentPage + 1 %>, '<%= sortBy %>')" aria-label="Next">
        <span aria-hidden="true">&raquo;</span>
        <span class="sr-only">Next</span>
    </a>
</li>